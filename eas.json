{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "GEMINI_API_KEY": "$GEMINI_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "preview",
        "GEMINI_API_KEY": "$GEMINI_API_KEY"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "NODE_ENV": "production",
        "GEMINI_API_KEY": "$GEMINI_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
